          "Specify mpls tunnel can be fast-rerouted";
        presence "true";
        leaf bw-protect {
          description
            "bandwidth protection desired";
          type empty;
        }
        leaf node-protect {
          description
            "node protection desired";
          type empty;
        }
      }
      container forwarding-adjacency {
        description
          "Treat this tunnel as a Forwarding Adjacency";
        presence "true";
        leaf holdtime {
          description
            "How long to wait upon flooding a down Forwarding Adjacency: <0..4294967295> msec";
          type string;
        }
      }
      container interface {
        description
          "Add tunnel interface config";
        container down {
          description
            " Tunnel interface down config";
          leaf delay {
            description
              "Tunnel interface down delay config: <0..0>";
            type string;
            // NCS drop must statement
          }
        }
      }
      leaf load-share {
        description
          "Specify tunnel load-sharing metric: <0..1000000>";
        type string;
      }
      leaf name {
        description
          "specify a name describing the TE tunnel";
        type string;
      }
      container path-option {
        list working {
          // NCS patched for predictable order
          ordered-by user;
          key "id";
          leaf id {
            type string;
          }
          choice path-option-choice {
            // NCS drop mandatory statement
            case dynamic-option {
              container dynamic {
                description
                  "Setup based on dynamically calculated path";
                presence "true";
                choice popt-choice {
                  case option-segment-routing {
                    container segment-routing {
                      description
                        "Specify segment-routing for this pathoption";
                      presence "true";
                      choice popt-sr-choice {
                        case option-attributes {
                          uses ncs_template_mpls-te-popt-attributes-grouping;
                        }
                        case option-lockdown {
                          uses ncs_template_mpls-te-popt-lockdown-grouping;
                        }
                        case option-pce {
                          uses ncs_template_mpls-te-popt-pce-verbatim-grouping;
                        }
                      }
                    }
                  }
                  case option-pce {
                    uses ncs_template_mpls-te-popt-pce-verbatim-grouping;
                  }
                  case option-lockdown {
                    uses ncs_template_mpls-te-popt-lockdown-grouping;
                    uses ncs_template_mpls-te-popt-bandwidth-grouping;
                  }
                  case option-attibutes {
                    uses ncs_template_mpls-te-popt-attributes-grouping;
                  }
                }
              }
            }
            case explicit-option {
              container explicit {
                description
                  "Setup based on preconfigured path";
                uses ncs_template_mpls-te-popt-id-grouping;
                uses ncs_template_mpls-te-popt-segment-routing-grouping;
                uses ncs_template_mpls-te-popt-verbatim-grouping;
                choice popt-choice {
                  case attributes-option {
                    uses ncs_template_mpls-te-popt-attributes-grouping;
                  }
                  case lockdown-option {
                    uses ncs_template_mpls-te-popt-lockdown-grouping;
                    uses ncs_template_mpls-te-popt-bandwidth-grouping {
                      // NCS drop when statement
                    }
                  }
                }
              }
            }
            case segment-routing-option {
              container segment-routing {
                description
                  "Specify segment-routing for this pathoption";
                presence "true";
                uses ncs_template_mpls-te-popt-lockdown-grouping;
              }
            }
          }
        }
        list protect {
          // NCS patched for predictable order
          ordered-by user;
          key "id";
          leaf id {
            type string;
          }
          choice protect-choice {
            // NCS drop mandatory statement
            case diverse-option {
              container diverse {
                description
                  "Setup based on dynamically calculated diverse path";
                leaf node {
                  description
                    "Use at least node diverse path";
                  type empty;
                }
                leaf srlg {
                  description
                    "Use at least srlg diverse path";
                  type empty;
                }
                uses ncs_template_mpls-te-popt-segment-routing-grouping;
                leaf non-revertive {
                  description
                    "Diverse non-revertive";
                  type empty;
                }
                uses ncs_template_mpls-te-popt-pce-verbatim-grouping;
                leaf lockdown {
                  description
                    "Not a candidate for reoptimization";
                  type empty;
                }
                uses ncs_template_mpls-te-popt-attributes-bw-grouping {
                  // NCS drop when statement
                }
                // NCS drop must statement
              }
            }
            case explicit-option {
              container explicit {
                description
                  "Setup based on preconfigured path";
                uses ncs_template_mpls-te-popt-id-grouping;
                uses ncs_template_mpls-te-popt-segment-routing-grouping;
                leaf non-revertive {
                  description
                    "Diverse non-revertive";
                  type empty;
                }
                uses ncs_template_mpls-te-popt-verbatim-grouping;
                uses ncs_template_mpls-te-popt-attributes-bw-grouping;
              }
            }
            case list-option {
              container list {
                description
                  "A list of backup paths";
                uses ncs_template_mpls-te-popt-id-grouping;
                uses ncs_template_mpls-te-popt-segment-routing-grouping;
                leaf non-revertive {
                  description
                    "Make the protect path non-revertive";
                  type empty;
                }
                uses ncs_template_mpls-te-popt-attributes-bw-pce-grouping;
              }
            }
          }
        }
      }
      uses ncs_template_mpls-te-path-selection-grouping {
        augment "path-selection" {
          leaf cost-limit {
            description
              "Cost Limit Configuration: <1..4294967295>";
            type string;
          }
          leaf hop-limit {
            description
              "Cost Limit Configuration: <1..255>";
            type string;
          }
          leaf tiebreaker {
            description
              "Tiebreaker Configuration: < max-fill | min-fill | random >";
            type string;
          }
        }
      }
      uses ncs_template_mpls-te-setup-hold-priority-grouping {
        refine "priority" {
          description
            "Tunnel priority";
        }
        refine "priority/hold-priority" {
          // NCS drop must statement
        }
      }
      container record-route {
        description
          "record the route used by the tunnel";
        presence "true";
      }
    }
