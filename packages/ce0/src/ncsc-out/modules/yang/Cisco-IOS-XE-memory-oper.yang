  augment "/ncs:devices/ncs:device/ncs:config" {
    // NCS drop config false container 'memory-statistics'
  }
  augment "/ncs:devices/ncs:device/ncs:live-status" {
    container memory-statistics {
      tailf:ncs-device-type "netconf";
      tailf:display-when "not(../../ncs:module) or ../../ncs:module[ncs:name='Cisco-IOS-XE-memory-oper'] or (../../ncs:live-status-protocol[not(ncs:capability)]) or (../../ncs:live-status-protocol/ncs:capability/ncs:module[.='Cisco-IOS-XE-memory-oper'])";
      description
        "Data nodes for All Memory Pool Statistics.";
      list memory-statistic {
        key "name";
        description
          "The list of software memory pools in the system.";
        uses memory-ios-xe-oper:live_ncs_memory-stats;
      }
    }
  }
  augment "/ncs:devices/ncs:template/ncs:config" {
    // NCS drop config false container 'memory-statistics'
  }
