    container voice {
      description
        "Global voice configuration";
      list service {
        // NCS patched for predictable order
        ordered-by user;
        description
          "Global packet telephony service commands";
        key "type";
        leaf type {
          type string;
        }
        container fax {
          description
            "Global fax commands";
          leaf protocol {
            description
              "The system-wide fax protocol type";
            type string;
          }
          leaf fallback {
            description
              "fax transport to use if T.38 unsupported on far end";
